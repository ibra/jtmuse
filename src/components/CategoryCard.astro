---
interface Props {
  category: string;
}

const { category } = Astro.props;
---

<a href=`/studyguides/${category}.pdf`>
  <img
    src=`categories/${category}.jpg`
    alt={category}
    class="w-2xl h-auto rounded-2xl transition duration-150 ease-in-out transform hover:blur-sm md:max-w-64 sm:max-w-32"
  />
</a>
