---
interface Props {
  href: string;
  category: string;
}

const { href, category } = Astro.props;
---

<a href={href}>
  <img
    src=`assets/categories/${category}.webp`
    alt="Description"
    class="w-2xl h-auto rounded-lg transition duration-300 ease-in-out transform hover:blur-md"
  />
</a>
